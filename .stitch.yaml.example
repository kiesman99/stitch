# Example configuration file for stitch
# Copy this to ~/.stitch.yaml or specify with --config

# Default output settings
output: ""
format: "png"
worldfile: false

# Tile settings
tilesize: 256

# HTTP settings
user-agent: "stitch/2.0.0"

# Common tile server URLs (for reference)
# OpenStreetMap: http://a.tile.openstreetmap.org/{z}/{x}/{y}.png
# Stamen Watercolor: http://b.tile.stamen.com/watercolor/{z}/{x}/{y}.jpg
# Stamen Toner: http://b.tile.stamen.com/toner/{z}/{x}/{y}.png